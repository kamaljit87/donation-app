# Production Environment Variables for React Frontend

# Backend API URL
REACT_APP_API_URL=https://prabhupadannakshetra.org/api

# Razorpay Key ID - IMPORTANT: Update with your actual Razorpay key from dashboard
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_id

# App Name
REACT_APP_NAME=DonationApp

# Build Optimization for Low Memory (1GB RAM)
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0
DISABLE_ESLINT_PLUGIN=true
TSC_COMPILE_ON_ERROR=true
DISABLE_NEW_JSX_TRANSFORM=true
FAST_REFRESH=false
